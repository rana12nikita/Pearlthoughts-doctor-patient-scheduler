    "@types/passport-local": "^1.0.35",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
